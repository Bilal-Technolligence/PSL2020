                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:layout_weight="4"
                    android:padding="5dp"
                    android:layout_alignParentLeft="true"
                    android:orientation="vertical">

                    <androidx.cardview.widget.CardView
                        android:id="@+id/outCard"
                        android:layout_width="70dp"
                        android:layout_height="45dp"
                        android:layout_weight="5"
                        android:layout_marginTop="10dp"
                        android:padding="5dp"
                        android:layout_marginLeft="10dp"
                        app:cardBackgroundColor="@color/colorRed"
                        app:cardCornerRadius="20dp">

                        <TextView
                            android:id="@+id/txtOut"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:paddingLeft="15dp"
                            android:paddingTop="10dp"
                            android:text="Out"
                            android:textColor="@color/colorWhite"
                            android:textSize="20dp" />


                    </androidx.cardview.widget.CardView>
                    <androidx.cardview.widget.CardView
                        android:id="@+id/oneCard"
                        android:layout_width="70dp"
                        android:layout_height="45dp"
                        android:layout_weight="5"
                        android:padding="5dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="90dp"
                        app:cardBackgroundColor="@color/colorBlue"
                        app:cardCornerRadius="20dp">

                        <TextView
                            android:id="@+id/txtone"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:paddingLeft="28dp"

                            android:paddingTop="10dp"
                            android:text="1"
                            android:textColor="@color/colorWhite"
                            android:textSize="20dp" />


                    </androidx.cardview.widget.CardView>
                    <androidx.cardview.widget.CardView
                        android:id="@+id/twoCard"
                        android:layout_width="70dp"
                        android:layout_height="45dp"
                        android:layout_weight="5"
                        android:padding="5dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="170dp"
                        app:cardBackgroundColor="@color/colorBlue"
                        app:cardCornerRadius="20dp">

                        <TextView
                            android:id="@+id/txttwo"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:paddingLeft="28dp"

                            android:paddingTop="10dp"
                            android:text="2"
                            android:textColor="@color/colorWhite"
                            android:textSize="20dp" />


                    </androidx.cardview.widget.CardView>
                    <androidx.cardview.widget.CardView
                        android:id="@+id/threeCard"
                        android:layout_width="70dp"
                        android:layout_height="45dp"
                        android:layout_weight="5"
                        android:padding="5dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="250dp"
                        app:cardBackgroundColor="@color/colorBlue"
                        app:cardCornerRadius="20dp">

                        <TextView
                            android:id="@+id/txtthree"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:paddingLeft="28dp"

                            android:paddingTop="10dp"
                            android:text="3"
                            android:textColor="@color/colorWhite"
                            android:textSize="20dp" />


                    </androidx.cardview.widget.CardView>
                    <androidx.cardview.widget.CardView
                        android:id="@+id/wideCard"
                        android:layout_width="70dp"
                        android:layout_height="45dp"
                        android:layout_weight="5"
                        android:padding="5dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="330dp"
                        app:cardBackgroundColor="@color/colorBlue"
                        app:cardCornerRadius="20dp">

                        <TextView
                            android:id="@+id/txtwide"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:paddingLeft="15dp"

                            android:paddingTop="10dp"
                            android:text="wide"
                            android:textColor="@color/colorWhite"
                            android:textSize="20dp" />


                    </androidx.cardview.widget.CardView>



                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/points2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="90dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_marginBottom="5dp"
                    android:layout_weight="4"
                    android:padding="5dp"
                    android:layout_alignParentLeft="true"
                    android:orientation="vertical">

                    <androidx.cardview.widget.CardView
                        android:id="@+id/fourCard"
                        android:layout_width="70dp"
                        android:layout_height="45dp"
                        android:layout_weight="5"
                        android:layout_marginTop="10dp"
                        android:padding="5dp"
                        android:layout_marginLeft="10dp"
                        app:cardBackgroundColor="@color/colorGreen"
                        app:cardCornerRadius="20dp">

                        <TextView
                            android:id="@+id/txtfour"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:paddingLeft="28dp"
                            android:paddingTop="10dp"
                            android:text="4"
                            android:textColor="@color/colorWhite"
                            android:textSize="20dp" />


                    </androidx.cardview.widget.CardView>
                    <androidx.cardview.widget.CardView
                        android:id="@+id/sixCard"
                        android:layout_width="70dp"
                        android:layout_height="45dp"
                        android:layout_weight="5"
                        android:padding="5dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="90dp"
                        app:cardBackgroundColor="@color/colorGreen"
                        app:cardCornerRadius="20dp">

                        <TextView
                            android:id="@+id/txsix"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:paddingLeft="28dp"

                            android:paddingTop="10dp"
                            android:text="6"
                            android:textColor="@color/colorWhite"
                            android:textSize="20dp" />


                    </androidx.cardview.widget.CardView>
                    <androidx.cardview.widget.CardView
                        android:id="@+id/dotCard"
                        android:layout_width="70dp"
                        android:layout_height="45dp"
                        android:layout_weight="5"
                        android:padding="5dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="170dp"
                        app:cardBackgroundColor="@color/colorBlue"
                        app:cardCornerRadius="20dp">

                        <TextView
                            android:id="@+id/txtdot"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:paddingLeft="20dp"

                            android:paddingTop="10dp"
                            android:text="dot"
                            android:textColor="@color/colorWhite"
                            android:textSize="20dp" />


                    </androidx.cardview.widget.CardView>
                    <androidx.cardview.widget.CardView
                        android:id="@+id/noballCard"
                        android:layout_width="70dp"
                        android:layout_height="45dp"
                        android:layout_weight="5"
                        android:padding="5dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="250dp"
                        app:cardBackgroundColor="@color/colorBlue"
                        app:cardCornerRadius="20dp">

                        <TextView
                            android:id="@+id/txtno"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:paddingLeft="10dp"

                            android:paddingTop="10dp"
                            android:text="noball"
                            android:textColor="@color/colorWhite"
                            android:textSize="20dp" />


                    </androidx.cardview.widget.CardView>
                    <androidx.cardview.widget.CardView
                        android:id="@+id/runoutCard"
                        android:layout_width="70dp"
                        android:layout_height="45dp"
                        android:layout_weight="5"
                        android:padding="5dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="330dp"
                        app:cardBackgroundColor="@color/colorRed"
                        app:cardCornerRadius="20dp">

                        <TextView
                            android:id="@+id/txtrunout"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:paddingLeft="8dp"

                            android:paddingTop="10dp"
                            android:text="runout"
                            android:textColor="@color/colorWhite"
                            android:textSize="20dp" />


                    </androidx.cardview.widget.CardView>

                </RelativeLayout>






            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:id="@+id/newsCard2"
                android:layout_height="190dp"
                android:layout_margin="5dp"
                android:layout_width="match_parent"
                android:layout_weight="10"
                app:cardCornerRadius="30dp"
                android:layout_below="@+id/predicitionCard"
                android:padding="5dp">


                <LinearLayout
                    android:layout_width="120dp"
                    android:layout_height="150dp"
                    android:padding="10dp"
                    android:orientation="vertical"
                    android:layout_alignParentLeft="true"
                    >

                    <androidx.cardview.widget.CardView
                        android:id="@+id/newsVideoCard2"
                        android:layout_height="match_parent"
                        android:layout_margin="5dp"
                        android:layout_width="120dp"
                        android:layout_weight="10"

                        android:padding="10dp"
                        app:cardCornerRadius="30dp">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/pslne"
                            />
                    </androidx.cardview.widget.CardView>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="150dp"
                    android:orientation="vertical"
                    android:padding="10dp"
                    android:layout_marginLeft="120dp"
                    android:gravity="center"
                    android:elevation="16dp">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="PSL 2020 Squads, Online Tickets Time for Pakistan Super...."
                        android:textStyle="bold"
                        android:textColor="@color/colorDarkBlue"
                        android:id="@+id/notification_title1"
                        />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Now, all the players are geared up for PSL 2020. Now hopes are further mounting that as the security environment in Pakistan has rapidly been .. ..."
                        android:id="@+id/notification_description1"
                        android:textColor="#000000"

                        />

                    <TextView
                        android:id="@+id/notification_datetime1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="#000000"
                        android:layout_marginLeft="0dp"
                        android:layout_marginRight="110dp"

                        android:text="09/01/2020"
                        android:textSize="10dp" />
                </LinearLayout>
                <Button
                    android:layout_width="70dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="270dp"
                    android:textStyle="bold"
                    android:textSize="16dp"
                    android:paddingTop="17dp"
                    android:backgroundTint="#ffffff"
                    android:textColor="@color/colorDarkBlue"
                    android:text="More"
                    android:layout_gravity="bottom"/>

                <ImageView
                    android:layout_width="70dp"
                    android:layout_height="40dp"
                    android:layout_gravity="bottom"
                    android:layout_marginLeft="330dp"
                    android:src="@drawable/ic_arrow_forward_black_24dp" />


            </androidx.cardview.widget.CardView>

            <!--                <androidx.recyclerview.widget.RecyclerView-->
            <!--                    android:id="@+id/nList"-->
            <!--                    android:layout_width="match_parent"-->
            <!--                    android:layout_height="661dp"-->
            <!--                    android:layout_above="@+id/navigationView"-->
            <!--                    android:layout_alignParentEnd="true"-->
            <!--                    android:layout_marginEnd="0dp"-->
            <!--                    android:layout_marginBottom="14dp" />-->


        </RelativeLayout>
    </ScrollView>

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/navigationView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_marginStart="0dp"
        app:labelVisibilityMode="labeled"
        android:layout_marginEnd="0dp"
        android:background="#ffffff"
        app:itemTextColor="@color/colorDarkBlue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_alignParentBottom="true"
        app:menu="@menu/navigation"/>


</RelativeLayout>