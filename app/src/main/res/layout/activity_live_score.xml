                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:gravity="center"
                            android:text="3rd Match"
                            android:textColor="@color/colorBlue"
                            android:textSize="12dp" />

                        <androidx.cardview.widget.CardView
                            android:layout_width="40dp"
                            android:layout_height="15dp"
                            app:cardBackgroundColor="@color/colorRed"
                            android:layout_marginLeft="60dp"
                            android:layout_marginTop="5dp"

                            app:cardCornerRadius="10dp">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal"
                                android:text="Live"
                                android:gravity="center"
                                android:textColor="#ffffff"
                                android:textSize="10dp"
                                android:textStyle="bold" />

                        </androidx.cardview.widget.CardView>


                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/livematch2"
                        android:layout_width="340dp"
                        android:layout_height="110dp"
                        android:layout_below="@+id/live"
                        android:layout_weight="4"
                        android:layout_marginTop="30dp"
                        android:orientation="horizontal"
                        android:padding="10dp">

                        <LinearLayout
                            android:id="@+id/team_layout"
                            android:layout_width="70dp"
                            android:paddingTop="15dp"
                            android:layout_height="match_parent"
                            android:layout_alignParentRight="true"
                            android:layout_weight="1"
                            android:layout_marginRight="70dp"
                            android:orientation="vertical">
                            <TextView
                                android:id="@+id/team2ShortName"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textSize="14dp"
                                android:gravity="right"
                                android:padding="5dp"
                                android:textStyle="bold"
                                android:text="LQ"/>
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="right"
                                android:orientation="horizontal">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textSize="14dp"
                                    android:textStyle="bold"
                                    android:gravity="right"
                                    android:paddingLeft="2dp"
                                    android:textColor="@color/colorBlue"
                                    android:text="145-2"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textSize="7dp"
                                    android:padding="3dp"
                                    android:textStyle="bold"
                                    android:textColor="@color/colorBlack"
                                    android:text="17.3"/>
                            </LinearLayout>


                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/team_layout1"
                            android:layout_width="70dp"
                            android:layout_height="match_parent"
                            android:layout_alignParentLeft="true"
                            android:layout_weight="1"
                            android:paddingLeft="10dp"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/teamImage1"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="5dp"
                                android:paddingLeft="10dp"
                                android:src="@drawable/multansultan" />
                        </LinearLayout>


                        <LinearLayout
                            android:id="@+id/l2"
                            android:layout_width="70dp"
                            android:layout_height="match_parent"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="10dp"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <ImageView
                                android:id="@+id/t5"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="5dp"
                                android:paddingLeft="10dp"
                                android:src="@drawable/lahoreqalander" />

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/team_layout2"
                            android:layout_width="70dp"
                            android:layout_height="match_parent"
                            android:layout_alignParentLeft="true"
                            android:layout_marginLeft="70dp"
                            android:layout_weight="1"
                            android:paddingTop="15dp"
                            android:orientation="vertical">

                           <TextView
                               android:id="@+id/teamShortName"
                               android:layout_width="match_parent"
                               android:layout_height="wrap_content"
                               android:textSize="14dp"
                               android:padding="5dp"
                               android:textStyle="bold"
                               android:text="MS"/>
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:textSize="14dp"
                                    android:textStyle="bold"
                                    android:gravity="left"
                                    android:paddingLeft="2dp"
                                    android:textColor="@color/colorBlue"
                                    android:text="190-7"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textSize="7dp"
                                    android:textStyle="bold"
                                    android:padding="3dp"
                                    android:textColor="@color/colorBlack"
                                    android:text="20.0"/>
                            </LinearLayout>


                        </LinearLayout>

                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/matchresult"
                        android:layout_width="match_parent"
                        android:layout_marginTop="135dp"
                        android:layout_height="30dp"
                        android:layout_below="@+id/livematch2"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/txtmatches1"
                            android:layout_width="wrap_content"
                            android:layout_height="20dp"
                            android:layout_marginTop="5dp"
                            android:paddingTop="2dp"
                            android:paddingLeft="30dp"
                            android:gravity="center"
                            android:text="Lahore Qalander Needs 45 run on 15 balls to win"
                            android:textColor="@color/colorBlack"
                            android:textSize="13dp" />


                    </RelativeLayout>
                </androidx.cardview.widget.CardView>
            </RelativeLayout>
            <LinearLayout
                android:id="@+id/videosLayout"
                android:orientation="horizontal"
                android:layout_margin="10dp"
                android:layout_weight="4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/topLayout">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Vote Your Favourite Team"
                    android:paddingLeft="15dp"
                    android:textColorHint="@color/colorBlack"
                    android:textSize="25dp"
                    android:textColor="@color/colorBlack"/>
            </LinearLayout>
            <RelativeLayout
                android:id="@+id/vote_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="7dp"
                android:layout_below="@+id/videosLayout">

                <LinearLayout
                    android:id="@+id/team_rating_layout2"
                    android:layout_width="77dp"
                    android:layout_height="match_parent"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="70dp"
                    android:paddingTop="15dp"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/team_rating"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="25dp"
                        android:padding="5dp"
                        android:gravity="center"
                        android:textStyle="bold"
                        android:textColor="@color/colorDarkBlue"
                        android:text="MS"/>



                </LinearLayout>

                <androidx.cardview.widget.CardView
                    android:id="@+id/pridicition"
                    android:layout_width="80dp"
                    android:layout_height="70dp"
                    android:layout_marginTop="20dp"
                    android:paddingTop="20dp"
                    android:paddingLeft="30dp"
                    android:layout_marginLeft="70dp"
                    android:layout_below="@+id/team_rating_layout2"
                    android:layout_marginBottom="5dp"
                    android:layout_weight="10"
                    app:cardBackgroundColor="@color/colorGreen"
                    app:cardCornerRadius="40dp">

                    <TextView
                        android:id="@+id/out"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:paddingLeft="22dp"

                        android:paddingTop="20dp"
                        android:text="70%"
                        android:textColor="#ffffff"
                        android:textSize="25dp" />


                </androidx.cardview.widget.CardView>

                <LinearLayout
                    android:id="@+id/team_rating_layout3"
                    android:layout_width="77dp"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="70dp"
                    android:paddingTop="15dp"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/team_rating2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textSize="25dp"
                        android:padding="5dp"
                        android:gravity="center"
                        android:textStyle="bold"
                        android:textColor="@color/colorDarkBlue"
                        android:text="LQ"/>



                </LinearLayout>

                <androidx.cardview.widget.CardView
                    android:id="@+id/ratingCard"
                    android:layout_width="80dp"
                    android:layout_height="70dp"
                    android:layout_marginTop="20dp"
                    android:paddingTop="20dp"
                    android:paddingRight="30dp"
                    android:layout_marginRight="70dp"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/team_rating_layout2"
                    android:layout_marginBottom="5dp"
                    android:layout_weight="10"
                    app:cardBackgroundColor="@color/colorRed"
                    app:cardCornerRadius="40dp">

                    <TextView
                        android:id="@+id/team1percentage2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:paddingLeft="22dp"

                        android:paddingTop="20dp"
                        android:text="30%"
                        android:textColor="#ffffff"
                        android:textSize="25dp" />


                </androidx.cardview.widget.CardView>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/predicition_layout"
                android:orientation="vertical"
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/vote_layout">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:text="Ball by Ball Predicition "
                    android:padding="2dp"
                    android:textSize="25dp"
                    android:textColorHint="@color/colorBlack"
                    android:textColor="@color/colorBlack"/>
            </RelativeLayout>


            <androidx.cardview.widget.CardView
                android:id="@+id/predicitionCard"
                android:layout_height="190dp"
                android:layout_margin="5dp"
                android:layout_width="match_parent"
                android:layout_weight="10"
                app:cardCornerRadius="30dp"
                android:layout_below="@+id/predicition_layout"
                android:padding="5dp">
                <RelativeLayout
